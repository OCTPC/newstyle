

    <section class="articles">
      <h2>最新の記事</h2>
      <!-- start of article -->
      <%#= page_entries_info @contents %>
      <% @contents.each do |content| %>
      <article>
        <div class="article-title">
          <h3><%= content.title =%></h3>
        </div>
        <div class="article-body">
          <%= content.summary =%>
        </div>
      </article>
      <% end %>

    </section>
    <!-- end of articles -->
    <div class="text-center">
      <%= paginate @contents %>
    </div>
    <!-- end of paganetion -->

